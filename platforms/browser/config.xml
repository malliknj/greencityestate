<?xml version='1.0' encoding='utf-8'?>
<widget id="com.phonegap.net" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>NewCityEstate</name>
    <description>
       NewCityEstate
    </description>
    <author email="mallik@njsys.net" href="http://mallik@njsys.net">
        NjSys Team
    </author>
    <content src="index.html" />
    <icon src="NJSysLogoMobile.png" />
    <allow-intent href="http://njsys.net" />
    <allow-intent href="https://fonts.googleapis.com" />
    <access origin="http://google.com" />
    <access origin="https://google.com" />
    <access origin="http://maps.google.com" />
    <access origin="http://*.google.com" />
    <access origin="content:///*" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="http://njsys.net/*" />
    <allow-intent href="*://*.njsys.net/*" />
    <allow-intent href="sms:*" />
    <allow-intent href="tel:*" />
    <allow-intent href="geo:*" />
    <preference name="android-minSdkVersion" value="14" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="orientation" value="portrait" />
    <preference name="android-build-tool" value="gradle" />
    <preference name="phonegap-version" value="cli-8.0.0" />
</widget>
